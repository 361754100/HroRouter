<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="black_overlay">#66000000</color>
    <color name="white_overlay">#FFFFFFFF</color>
    <declare-styleable name="ButtonBarContainerTheme"><attr format="reference" name="buttonBarStyle"/><attr format="reference" name="buttonBarButtonStyle"/></declare-styleable>
    <dimen name="activity_horizontal_margin">16dp</dimen>
    <dimen name="activity_vertical_margin">16dp</dimen>
    <string name="alarm_dist">预警范围：</string>
    <string name="alarm_dist_hint">请输入预警范围(单位：米)</string>
    <string name="alarm_phone">联动号码：</string>
    <string name="alarm_phone_hint">请输入联动号码</string>
    <string name="app_name">HroRouter</string>
    <string name="dummy_button">Dummy Button</string>
    <string name="dummy_content">DUMMY\nCONTENT</string>
    <string name="map_keywords">请输入目的地</string>
    <string name="myLocation_button">MLoc</string>
    <string name="settingSave">保存设置</string>
    <string name="tim_account">云信帐号：</string>
    <string name="tim_account_hint">请输入云信帐号</string>
    <string name="title_activity_map_settings">MapSettingsActivity</string>
    <string name="title_activity_settings">参数设置</string>
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.

        -->
    </style>
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <style name="ButtonBar">
        <item name="android:paddingLeft">2dp</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingRight">2dp</item>
        <item name="android:paddingBottom">0dp</item>
        <item name="android:background">@android:drawable/bottom_bar</item>
    </style>
    <style name="ButtonBarButton"/>
    <style name="FullscreenSettingActionBarStyle" parent="android:Widget.Holo.Light.ActionBar">
        <item name="android:background">@color/black_overlay</item>
        <item name="android:color">@color/white_overlay</item>
    </style>
    <style name="FullscreenTheme" parent="android:Theme.NoTitleBar">
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowBackground">@null</item>
        <item name="buttonBarStyle">@style/ButtonBar</item>
        <item name="buttonBarButtonStyle">@style/ButtonBarButton</item>
    </style>
    <style name="FullscreenThemeActionBar" parent="android:Theme.DeviceDefault.Light">
        <item name="android:actionBarStyle">@style/FullscreenSettingActionBarStyle</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="buttonBarStyle">@style/ButtonBar</item>
        <item name="buttonBarButtonStyle">@style/ButtonBarButton</item>
    </style>
</resources>